  "main": "src/index.ts",
  "style": "style/tables.css",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./style/tables.css": "./style/tables.css"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/massifrg/prosemirror-tables-sections.git"
  },
  "maintainers": [
    "M. Farinella <massifrg@gmail.com>"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.16.0",
    "eslint-plugin-jest": "^26.2.2",
    "ist": "^1.1.1",
    "prettier": "^2.6.2",
		"prosemirror-commands": "^1.6.1",
		"prosemirror-menu": "^1.2.4",
		"prosemirror-keymap": "^1.2.2",
    "prosemirror-schema-basic": "^1.2.1",
    "prosemirror-test-builder": "^1.1.0",
    "tsup": "^8.3.5",
    "typescript": "^4.0.0",
    "vite": "^6.0.11",
    "vitest": "^3.1.1"
  },
  "scripts": {
    "dev": "vite demo",
    "build_demo": "vite build demo",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
